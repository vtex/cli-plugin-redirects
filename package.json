{
  "name": "@vtex/cli-plugin-redirects",
  "description": "vtex plugin redirects",
  "version": "1.0.1",
  "type": "module",
  "bugs": "https://github.com/vtex/cli-plugin-redirects/issues",
  "dependencies": {
    "@oclif/core": "^3.26.0",
    "@tiagonapoli/oclif-plugin-spaced-commands": "^0.0.6",
    "@vtex/api": "3.77.0",
    "ajv": "^8.12.0",
    "csvtojson": "^2.0.10",
    "fs-extra": "^11.2.0",
    "json-array-split": "^1.0.0",
    "json2csv": "^5.0.7",
    "ora": "^5.4.1",
    "progress": "^2.0.3",
    "ramda": "^0.30.0",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.2.0",
    "oclif": "^4.7.0",
    "@types/fs-extra": "^11.0.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.0",
    "@types/json2csv": "^5.0.1",
    "@types/node": "^20.0.0",
    "@types/progress": "^2.0.7",
    "@types/ramda": "0.30.0",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5.2.0",
    "eslint-config-oclif-typescript": "^3.1.11",
    "globby": "^11.1.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.0",
    "nodemon-notifier-cli": "https://github.com/Slessi/nodemon-notifier-cli.git",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.9.2",
    "vtex": "4.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "/build",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/vtex/cli-plugin-redirects",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "bin": "oclif-example",
    "dirname": "oclif-example",
    "commands": "./build/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "redirects": {
        "description": "Imports, exports, or removes redirects from the current account and workspace. Run 'vtex redirects' to see all 3 subcommands."
      }
    }
  },
  "repository": "vtex/cli-plugin-redirects",
  "scripts": {
    "build-clean": "rm -rf build",
    "build-incremental": "tsc --incremental",
    "build": "yarn build-clean && yarn tsc && yarn oclif manifest",
    "ci:prettier-check": "prettier --check --config ./.prettierrc \"./src/**/*.{ts,tsx,js,jsx,json}\"",
    "ci:test": "yarn test --ci",
    "format-lint": "yarn format && yarn lint",
    "format": "prettier --config ./.prettierrc --write \"**/*.{ts,js,json}\"",
    "lint": "eslint ./src --cache --ext .ts --config .eslintrc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf build && tsc -b && oclif manifest && oclif readme",
    "prepublishOnly": "bash ./scripts/publishLock.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__ --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:export": "jest --testPathPattern=export",
    "version": "oclif readme && git add README.md",
    "watch": "yarn build-clean && yarn nodemon"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
